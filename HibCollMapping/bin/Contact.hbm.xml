<?xml version = "1.0" encoding = "utf-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
   <class name = "com.hibcoll.Contact" table = "usercontactdetail">
   <meta attribute= "class-description">This class contains contact details.</meta>
    
      <id name = "phoneid" type="int" column="phoneid">
         <generator class="native"></generator>
      </id>
      
      <property name = "phonenumber" type="string" length="15" column="phonenumber">
      </property>
   </class>
</hibernate-mapping>